<?xml version="1.0" encoding="UTF-8"?>
<strings>
	<language name="ru" full_name="Русский" description="clopper@novikovi.info">
		<frames>
			<main>
				<title>Тэги MP3</title>
				<load_list>Список файлов</load_list>
				<table>
					<column_file>Файл</column_file>
					<column_name>Название трека</column_name>
					<column_artist>Исполнитель</column_artist>
					<column_album>Альбом</column_album>
					<column_number>Номер трека</column_number>
				</table>
			</main>
		</frames>
		<dialog>
			<buttons>
				<ok>Ok</ok>
				<cancel>Отмена</cancel>
				<yes>Да</yes>
				<no>Нет</no>
				<yes_to_all>Да для всех</yes_to_all>
				<no_to_all>Нет для всех</no_to_all>
				<close>Закрыть</close>
				<choose>Выбрать</choose>
			</buttons>
			<preferences>
				<title>Настройки</title>
				<interface>Интерфейс</interface>
				<language>Язык (Language)</language>
				<font>Шрифт</font>
			</preferences>
			<language>
				<label>Выберите язык (Choose language)</label>
			</language>
			<font>
				<label_comp>Выберите шрифт для текстовых полей</label_comp>
				<label_text>Выберите шрифт для исходного текста</label_text>
				<name>Название</name>
				<size>Размер</size>
			</font>
			<wait>
				<title>Подождите</title>
			</wait>
			<open_file>
				<title>Открыть</title>
				<ok_button>Открыть</ok_button>
			</open_file>
		</dialog>
		<message>
			<error_title>Ошибка</error_title>
			<confirmation_title>Подтверждение</confirmation_title>
			<warning_title>Предупреждение</warning_title>
			<cannot_read_file>Не удалось прочесть файл '%s'</cannot_read_file>
		</message>
    </language>
    <language name="en" full_name="English" description="clopper@novikovi.info">
